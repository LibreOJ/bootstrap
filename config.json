{
  "env": {
    "title": "LibreOJ",
    "uiVersion": "0.2.8",

    "cdnEndpoint": "https://cdn.jsdelivr.net",
    "cdnEndpointCN": "https://cdn.menci.xyz",
    "cdnRoot": "${cdnEndpoint}/npm/@lyrio/ui@${uiVersion}/build",
    "cdnRootCN": "${cdnEndpointCN}/npm/@lyrio/ui@${uiVersion}/build",

    "apiEndpoint": "https://api.loj.ac",
    "apiEndpointCN": "https://api.loj.ac.cn",

    "bootstrapCdnRoot": "loj.ac",
    "bootstrapCdnRootCN": "bootstrap.loj.ac.cn",
    "bootstrapCdnRootStaging": "",
    "bootstrapCdnRootStagingCN": "bootstrap-staging.loj.ac.cn",

    "polyfillService": "https://polyfill.io",
    "polyfillServiceCN": "https://api.loj.ac.cn/polyfill",

    "assets": "${cdnEndpoint}/gh/libreoj/assets",
    "assetsCN": "${cdnEndpointCN}/gh/libreoj/assets",
    "favicon": "${assets}/favicon.ico",
    "faviconCN": "${assetsCN}/favicon.ico",
    "appLogo": "${assets}/logo-website.min.svg",
    "appLogoCN": "${assetsCN}/logo-website.min.svg",

    "gravatar": "https://gravatar.com",
    "gravatarCN": "https://cdn.menci.xyz",
    "ghAvatar": "https://github.com",
    "ghAvatarCN": "https://cdn.menci.xyz/gh-avatar",

    "twemoji": "https://twemoji.maxcdn.com/v/latest/",
    "twemojiCN": "https://lf9-cdn-tos.bytecdntp.com/cdn/expire-1-y/twemoji/13.0.1/"
  },
  "htmlTemplate": "${cdnRoot}/index.html",
  "substitution": {
    "__api_endpoint__": {
      "CN": "${apiEndpointCN}",
      "": "${apiEndpoint}"
    },
    "__polyfill_service__": {
      "CN": "${polyfillServiceCN}",
      "": "${polyfillService}"
    },
    "__default_title__": "${title}",
    "__public_path__": {
      "CN": "${cdnRootCN}",
      "": "${cdnRoot}"
    },
    "__favicon__": {
      "CN": "${faviconCN}",
      "": "${favicon}"
    },
    "__applogo__": {
      "CN": "${appLogoCN}",
      "": "${appLogo}"
    },
    "__gravatar__": {
      "CN": "${gravatarCN}",
      "": "${gravatar}"
    },
    "__ghavatar__": {
      "CN": "${ghAvatarCN}",
      "": "${ghAvatar}"
    },
    "__twemoji__": {
      "CN": "${twemojiCN}",
      "": "${twemoji}"
    },
    "__bootstrap_cdn_root__": {
      "CN": "${bootstrapCdnRootCN}",
      "": "${bootstrapCdnRoot}"
    },
    "__bootstrap_cdn_root_staging__": {
      "CN": "${bootstrapCdnRootStagingCN}",
      "": "${bootstrapCdnRootStaging}"
    }
  }
}
