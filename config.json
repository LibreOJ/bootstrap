{
  "env": {
    "title": "LibreOJ",

    "cdnEndpoint": "https://cdn.menci.xyz",
    "cdnRoot": "${cdnEndpoint}/npm/@lyrio/ui@0.2.5/build",

    "apiEndpointCN": "https://api.loj.ac.cn",
    "apiEndpoint": "https://api.loj.ac",

    "polyfillServiceCN": "https://api.loj.ac.cn/polyfill",
    "polyfillService": "https://polyfill.io",

    "favicon": "${cdnEndpoint}/gh/libreoj/assets/favicon.ico"
  },
  "htmlTemplate": "${cdnRoot}/index.html",
  "substitution": {
    "__api_endpoint__": {
      "CN": "${apiEndpointCN}",
      "": "${apiEndpoint}"
    },
    "__polyfill_service__": {
      "CN": "${polyfillServiceCN}",
      "": "${polyfillService}"
    },
    "__default_title__": "${title}",
    "__public_path__": "${cdnRoot}",
    "__favicon__": "${favicon}"
  }
}
