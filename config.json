{
  "env": {
    "title": "LibreOJ",
    "uiVersion": "0.2.6",

    "cdnEndpoint": "https://cdn.jsdelivr.net",
    "cdnEndpointCN": "https://cdn.menci.xyz",
    "cdnRoot": "${cdnEndpoint}/npm/@lyrio/ui@${uiVersion}/build",
    "cdnRootCN": "${cdnEndpointCN}/npm/@lyrio/ui@${uiVersion}/build",

    "apiEndpointCN": "https://api.loj.ac.cn",
    "apiEndpoint": "https://api.loj.ac",

    "polyfillServiceCN": "https://api.loj.ac.cn/polyfill",
    "polyfillService": "https://polyfill.io",

    "assets": "${cdnEndpoint}/gh/libreoj/assets",
    "assetsCN": "${cdnEndpointCN}/gh/libreoj/assets",
    "favicon": "${assets}/favicon.ico",
    "faviconCN": "${assetsCN}/favicon.ico",
    "appLogo": "${assets}/logo-website.min.svg",
    "appLogoCN": "${assetsCN}/logo-website.min.svg",

    "gravatar": "https://gravatar.com",
    "gravatarCN": "https://cdn.menci.xyz"
  },
  "htmlTemplate": "${cdnRoot}/index.html",
  "substitution": {
    "__api_endpoint__": {
      "CN": "${apiEndpointCN}",
      "": "${apiEndpoint}"
    },
    "__polyfill_service__": {
      "CN": "${polyfillServiceCN}",
      "": "${polyfillService}"
    },
    "__default_title__": "${title}",
    "__public_path__": {
      "CN": "${cdnRootCN}",
      "": "${cdnRoot}"
    },
    "__favicon__": {
      "CN": "${faviconCN}",
      "": "${favicon}"
    },
    "__applogo__": {
      "CN": "${appLogoCN}",
      "": "${appLogo}"
    },
    "__gravatar__": {
      "CN": "${gravatarCN}",
      "": "${gravatar}"
    }
  }
}
